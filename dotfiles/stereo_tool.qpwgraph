   <output node="stereo_tool" port="stereo_tool:out_r"/>
   <input node="butt-virtual-audio-device" port="butt-virtual-audio-device:playback_FR"/>
  </item>
  <item node-type="pipewire" port-type="pipewire-audio">
   <output node="stereo_tool" port="stereo_tool:out_l"/>
   <input node="butt-virtual-audio-device" port="butt-virtual-audio-device:playback_FL"/>
